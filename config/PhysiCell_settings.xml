						<uptake_rate units="1/min">0.0</uptake_rate>
					</substrate>
					<substrate name="anti-inflammatory cytokine">
						<secretion_target units="dimensionless substrate concentration">1</secretion_target>
						<uptake_rate units="1/min">0.0</uptake_rate>
					</substrate>
				</secretion>
				<molecular />
			</phenotype>
			<custom_data>
				<PDL1_expression description="PDL1 cell expression" type="double" units="dimensionless">0.0</PDL1_expression>
				<neoantigens_intracellular description="total neoantigens on cell" type="double" units="neoantigens">0</neoantigens_intracellular>
